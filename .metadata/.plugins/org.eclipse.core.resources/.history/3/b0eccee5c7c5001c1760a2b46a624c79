
public class For {

	public static void main(String[] args) {
		
		 System.out.println("---------------------");
	        
	        for(int i=1;i<=10;i++)  //1부터 10까지 반복
	        {
	            System.out.print(i);
	        }
	        System.out.println("");
	        System.out.println("---------------------");
	        
	        for(int j=1;j<=10;j++)
	        {
	            System.out.print(j+" ");
	        }
	        System.out.println("");
	        System.out.println("---------------------");
	        
	        for(int k=1;k<=10;k++)
	        {
	            System.out.println(k);
	        }
		
	}

}
