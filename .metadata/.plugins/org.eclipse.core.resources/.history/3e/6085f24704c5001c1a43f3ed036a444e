package DTO02;

public class AttachDTO {
	
	int idx;
	int boardIdx;
	String originalName;
	String saveName;
	int size;
	
	public void info() {
		System.out.println("idx = " + idx);
		System.out.println("boardIdx = " + boardIdx);
		System.out.println("originalName = " + originalName);
		System.out.println("saveName = " + saveName);
	}
}
