package Client;

import java.io.IOException;
import java.net.Socket;

public class ReceiveThread extends Thread {
	private final Socket socket;
	private String receiveString;

	// 매개변수 1개짜리 생성자
	public ReceiveThread(Socket socket) {
		this.socket = socket;
	}

	@Override
	public void run() {
		try {
			
		} catch (IOException e) {
			// 에러 출력
			e.printStackTrace();
		}
	}
	
	
	
}
